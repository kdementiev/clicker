{% extends '::base.html.twig' %}

{% block content %}
    <div class="panel panel-danger">
        <div class="panel-heading">
            <h3 class="panel-title">{{ 'page.click.error.title'|trans }}</h3>
        </div>
        <div class="panel-body">
            You click #[{{ click.id }}] <b>already done</b>!<br /><br />

            {% for flash_message in app.session.flashBag.get('redirect_notice') %}
                <div class="flash-notice">
                    {{ flash_message|raw }}
                </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}

